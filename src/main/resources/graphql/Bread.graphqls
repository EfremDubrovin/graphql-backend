type Bread {
	name: String!
	bakeTime: Int
	flourTypes: [FlourType]
	extraIngredients: ExtraIngredients
	bakery: Bakery
}

enum FlourType {
	ALL_PURPOSE
	ALMOND
	AMARANTH
	BARLEY
	BUCKWHEAT
}

# additional ingredients like olives, cheese, chocolate, etc
scalar ExtraIngredients

type Bakery {
	name: String!
}
